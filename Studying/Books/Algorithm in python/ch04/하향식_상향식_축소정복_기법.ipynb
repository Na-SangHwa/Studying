        "#하향식\n",
        "def factorial_recur(n):\n",
        "  if n == 1:\n",
        "    return 1\n",
        "  else :\n",
        "    return (n*factorial_recur(n-1))\n",
        "#상향식\n",
        "def factorial_iter(n):\n",
        "  result=1\n",
        "  for k in range(1,n+1):\n",
        "    result=result*k\n",
        "  return result\n",
        "\n",
        "print(factorial_recur(4))\n",
        "print(factorial_iter(4))\n",
        "\n"
      ]
    }
  ]
}